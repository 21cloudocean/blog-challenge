<%- include("partials/header") -%>
      <h1><%= pageTitle %></h1>
<form action="/compose" method="post">
<div class="form-group">
      <label for="textTitle">Title</label>
    <input class="form-control" id="textTitle" type="text" name="postTitle">
    <label for="textContent">Post</label>
    <textarea class="form-control" id="textContent" name="postBody" rows="8" cols="80"></textarea>
</div>
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer") -%>
